;; **
;;; ## 3. Basic analysis
;; **

;; **
;;; #### Plotting with `incanter.charts`
;; **

;; @@
(require '[incanter 
           [core :refer [view dataset]]
           [charts :refer [candle-stick-plot]]])

(def candles-dataset (dataset (vec (keys (first candles))) candles))

(def candles-chart (candle-stick-plot :data candles-dataset
                         :high :highMid
                         :low :lowMid
                         :open :openMid
                         :close :closeMid
                         :date :time))

(view candles-chart)
